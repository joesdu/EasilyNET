<?xml version="1.0" encoding="utf-8"?>

<Project>
    <PropertyGroup>
        <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
    </PropertyGroup>
    <ItemGroup>
        <None Include="..\..\LICENSE" Pack="true" PackagePath="" />
    </ItemGroup>
    <PropertyGroup>
        <TargetFrameworks>net8.0;net9.0</TargetFrameworks>
        <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
    </PropertyGroup>
    <PropertyGroup>
        <NoWarn>$(NoWarn);IDE0130;</NoWarn>
    </PropertyGroup>
    <ItemGroup>
        <PackageVersion Include="Backport.System.Threading.Lock" Version="2.0.5" />
        <PackageVersion Include="BouncyCastle.Cryptography" Version="2.5.0-beta.105" />
        <PackageVersion Include="MessagePack" Version="3.0.238-rc.1" />
        <PackageVersion Include="MongoDB.Driver" Version="3.0.0" />
        <PackageVersion Include="RabbitMQ.Client" Version="7.0.0" />
        <PackageVersion Include="Serilog" Version="4.1.0-dev-02238" />
        <PackageVersion Include="Spectre.Console.Json" Version="0.49.2-preview.0.47" />
        <PackageVersion Include="Swashbuckle.AspNetCore.SwaggerGen" Version="6.9.0" />
        <PackageVersion Include="Swashbuckle.AspNetCore.SwaggerUI" Version="6.9.0" />
        <!--microsoft asp.net core -->
        <PackageVersion Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.0-rc.2.24473.5" />
        <PackageVersion Include="Microsoft.Extensions.DependencyModel" Version="9.0.0-rc.2.24473.5" />
        <PackageVersion Include="Microsoft.Extensions.Hosting.Abstractions" Version="9.0.0-rc.2.24473.5" />
        <PackageVersion Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.0-rc.2.24473.5" />
        <PackageVersion Include="Microsoft.Extensions.Resilience" Version="9.0.0-preview.9.24507.7" />
        <PackageVersion Include="System.Diagnostics.DiagnosticSource" Version="9.0.0-rc.2.24473.5" />
    </ItemGroup>
</Project>
