{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "ServicesEnable": {
    "Swagger": true
  },
  "ConnectionStrings": {
    /// 当MongoDB副本集无法通过多个主机名或IP连接时,可仅连接主节点,并添加 &directConnection=true 参数如:
    /// mongodb://joe:a123456@host.docker.internal:27017/?replicaSet=rs0&authSource=admin&serverSelectionTimeoutMS=1000&directConnection=true
    "Mongo": "mongodb://joe:a123456@host.docker.internal:27017,host.docker.internal:27018,host.docker.internal:27019/?replicaSet=rs0&authSource=admin&serverSelectionTimeoutMS=1000",
    "Rabbit": "amqp://admin:a123456@host.docker.internal:5672/%2f",
    "Garnet": "host.docker.internal:6379,password=a123456"
  },
  "OpenTelemetry": {
    "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:4317",
    "DASHBOARD_OTLP_PRIMARYAPIKEY": "rVmuMdaqCEruWEbKANjmcIQMnKIhDiLUYSFHaZAVlMktmwDhMUAPIZyfizmoLuSwAePPVhhPigpJUIsAsPZcwfmaMnBxxRuxatrrHNSOKUxUVGFlYQtGtbqtOPasMvPd"
  }
}
